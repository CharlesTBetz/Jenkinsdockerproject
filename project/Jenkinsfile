pipeline {
     agent any
     stages {
          stage("checkout") {
               steps {
                 git url: 'https://github.com/mekulaventure/Jenkinsdockerproject.git'
               }
          }
          stage('deploy stage Stage') {
               steps {
                    sh 'def server = Artifactory.server 'artifactory server''
	             sh'  def uploadSpec = {
                def uploadSpec = """{
  		"files": [
    			{
      			"pattern": "target/hello-world",
            		"target": "MyArtRepo/${BUILD_NUMBER}/"
    				}
 				]
			}"""
		       server.upload(uploadSpec)
               }'
          }
       }
     }
}
