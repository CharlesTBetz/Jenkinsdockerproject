pipeline {
    agent { dockerfile true }
          
stages{
    stage('Clone repository') {
	steps {
        /* Let's make sure we have the repository cloned to our workspace */

        checkout "https://github.com/mekulaventure/Jenkinsdockerproject.git"
		}
	    stage('Build Image') {
		    steps {
			     docker.build("emma-hello")
		    }
	    }
	}
    }
}
